{
  "name": "template-electron-installation",
  "productName": "MyCoolApp",
  "version": "1.0.0",
  "description": "",
  "main": "./src/app.js",
  "license": "MIT",
  "author": {
    "name": "Matt DesLauriers",
    "email": "dave.des@gmail.com",
    "url": "https://github.com/mattdesl"
  },
  "dependencies": {
    "canvas-sketch": "^0.3.1"
  },
  "devDependencies": {
    "electron": "^5.0.3",
    "electron-packager": "^13.1.1",
    "http-server": "^0.11.1",
    "rimraf": "^2.6.3"
  },
  "scripts": {
    "app": "npm run bundle && electron .",
    "zip": "rimraf app.zip && zip -r -X app.zip -8 public/ build/app-darwin-x64/ build/app-win32-ia32/",
    "packager": "npm run bundle && rimraf build/ && electron-packager . --out=build --all --overwrite",
    "serve": "npm run bundle && http-server public/ -c-1 -p 9966",
    "start": "canvas-sketch src/sketch.js --dir=public --html=src/index.html --open",
    "bundle": "canvas-sketch src/sketch.js --dir=public --html=src/index.html --build --title=app --name=index"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/mattdesl/template-electron-installation.git"
  },
  "homepage": "https://github.com/mattdesl/template-electron-installation",
  "bugs": {
    "url": "https://github.com/mattdesl/template-electron-installation/issues"
  }
}
